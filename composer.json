{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "doctrine/dbal": "^4.0",
        "google-gemini-php/laravel": "^2.0",
        "google/apiclient": "^2.17",
        "guzzlehttp/guzzle": "^7.9",
        "inertiajs/inertia-laravel": "^2.0",
        "laravel/framework": "^12.0",
        "laravel/pennant": "^1.11",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.10",
        "maatwebsite/excel": "^3.1",
        "monolog/monolog": "^3.8",
        "opcodesio/log-viewer": "^3.11",
        "revolution/laravel-google-sheets": "^7.1",
        "spatie/laravel-backup": "^9.1",
        "spatie/laravel-data": "^4.18",
        "spatie/laravel-permission": "^6.22",
        "spatie/laravel-validation-rules": "^3.4",
        "tightenco/ziggy": "^2.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/breeze": "^2.2",
        "laravel/pint": "^1.18",
        "laravel/sail": "^1.36",
        "mockery/mockery": "^1.6",
        "mrdebug/crudgen": "^1.10",
        "nunomaduro/collision": "^8.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpunit/phpunit": "^11.0",
        "spatie/laravel-ignition": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "clnn": [
            "@php artisan optimize && @php artisan optimize:clear"
        ],
        "repull": [
            "git pull && @php artisan optimize && @php artisan optimize:clear && git log -1"
        ],
        "backup": [
            "php artisan backup:run && php artisan send:zip"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
